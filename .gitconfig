	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "main"]
	remote = origin
	merge = refs/heads/main
[remote "original"]
	url = git@github.com:AU-DIS/dm2021-exercises.git
	fetch = +refs/heads/*:refs/remotes/original/*
[pull]
    rebase = false
